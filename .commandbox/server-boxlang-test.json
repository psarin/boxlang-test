{
    "name":"boxlang-test",
    "profile":"development",
    "cfconfig":{
        "server":"./.cfconfig-server.json"
    },
    "openBrowser":false,
    "verboseErrors":true,
    "jvm":{
        "heapSize":2048,
        "minHeapSize":512,
        "javaHome":"${JAVA_HOME}",
        "javaVersion":"openjdk21_jdk"
    },
    "web":{
        "webroot":"./..",
        "gzipEnable":"true",
        "host":"localhost",
        "blockCFAdmin":false,
        "HTTP":{
            "enable":true,
            "port":8080
        },
        "SSL":{
            "enable":false,
            "port":443,
            "certFile":"",
            "keyFile":"",
            "keyPass":""
        },
        "rewrites":{
            "config":"./config/server-rewrites.xml",
            "configReloadSeconds":"30",
            "enable":"false"
        }
    },
    "env":{
        "BOXLANG_DEBUG":true
    },
    "dotenvFile":"../.docker/env/docker.env,../.docker/env/backend.common.env,../.docker/env/backend.production.env",
    "app":{
        "sessionCookieSecure":true,
        "sessionCookieHTTPOnly":true,
        "libDirs":"./config/lib",
        "engineConfigFile":"./boxlang.json"
    },
    "scripts":{
        "onServerInitialInstall":"install bx-ui-forms,bx-postgresql,bx-mail,bx-mysql,bx-derby,bx-compat-cfml,bx-web-support,bx-orm"
    }
}
